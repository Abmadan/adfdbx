{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdatabricksworkshop"
		},
		"AzureBlobStorageDataFlow_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageDataFlow'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureBlobStorageSource_linkedservice_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://adfstagingabhishek.blob.core.windows.net/"
		},
		"AzureDataLakeStorage_Destination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdestinationabhishek.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0120-065335-3wf849q7"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdatabricksworkshop_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageDataFlow_connectionString": {
						"value": "[parameters('AzureBlobStorageDataFlow_connectionString')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureBlobStorageSource_linkedservice_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorageSource_linkedservice_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureDataLakeStorage_Destination_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_Destination_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}